@model IEnumerable<LibraryProject.DataBase.Catagory>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Catagory Name")
        </th>
        <th>
            @Html.DisplayName("Description")
        </th>
     
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CatagoryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CatagoryID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CatagoryID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CatagoryID })
        </td>
    </tr>
}

</table>
