@model LibraryProject.DataBase.Student

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}

<h3>Are you sure you want to delete this?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Student Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentName)
        </dd>

        <dt>
            @Html.DisplayName("Student Registration Number")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentRegistrationNumber)
        </dd>

        <dt>
            @Html.DisplayName("Student Email Address")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentEmail)
        </dd>

        <dt>
            @Html.DisplayName("Student Passsword")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentPassword)
        </dd>

        <dt>
            @Html.DisplayName("Student Mobile Number")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudentMobileNumber)
        </dd>

        <dt>
            @Html.DisplayName("is Student Bann")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.isActive)
        </dd>

     
    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("Back to List", "Index")
        </div>
    }
</div>
