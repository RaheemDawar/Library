
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}


        <!-- start widget -->
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="row clearfix">
                    <div class="col-md-6 col-sm-6 col-12">
                        <div class="card">
                            <div class="panel-body">
                                <h3>Total Students</h3>
                               
                                <span class="text-small margin-top-10 full-width">@ViewBag.studentCount Count</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 col-12">
                        <div class="card">
                            <div class="panel-body">
                                <h3>Total Books</h3>
                               
                                <span class="text-small margin-top-10 full-width">@ViewBag.bookCount Count</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 col-12">
                        <div class="card">
                            <div class="panel-body">
                                <h3>Total Issued Books</h3>
                                
                                <span class="text-small margin-top-10 full-width">@ViewBag.issuedBookCount Count</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-6 col-sm-6 col-12">
                        <div class="card">
                            <div class="panel-body">
                                <h3>Total Places</h3>
                              
                                <span class="text-small margin-top-10 full-width">@ViewBag.placeCount Count </span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
         
        </div>
        <!-- end widget -->

        
   
